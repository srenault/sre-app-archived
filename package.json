{
  "name": "sre.dashboard",
  "displayName": "Dashboard",
  "version": "1.0.0",
  "description": "My personal dashboard.",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c && cordova build",
    "watch": "npm-run-all --parallel watch-www watch-src",
    "watch-www": "./cordova-watch-build",
    "watch-src": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "cordova run browser",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "srenault.contact@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "c3": "^0.7.0",
    "cordova-android": "^8.0.0",
    "cordova-browser": "6.0.0",
    "cordova-osx": "^5.0.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "date-fns": "^1.30.1",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-async": "^6.0.2",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "rxjs": "^6.5.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "browser",
      "android",
      "osx"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "cordova": "^9.0.0",
    "json-to-js": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size": "0.0.1",
    "rollup-plugin-uglify": "^6.0.2"
  }
