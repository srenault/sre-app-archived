<?xml version='1.0' encoding='utf-8'?>
<widget id="sre.dashboard" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Dashboard</name>
    <description>My personal dashboard</description>
    <author email="srenault.contact@gmail.com" href="https://github.com/srenault">
        Sebastien Renault
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <engine name="browser" spec="6.0.0" />
    <engine name="android" spec="^8.0.0" />
    <engine name="osx" spec="^5.0.0" />
</widget>
